{
  "name": "my-app",
  "version": "0.2.0",
  "author": "Your Name",
  "description": "React-TypeScript-Electron sample with Create React App and Electron Builder",
  "homepage": ".",
  "main": "build/electron/main.js",
  "private": true,
  "peerDependencies": {
    "@types/node": "^14.17.34"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.1",
    "electron-reload": "^1.5.0",
    "electron-settings": "^4.0.2",
    "mdb-reader": "^1.0.10",
    "node-adodb": "^5.0.3",
    "nodemon": "^2.0.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router": "5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "4.0.3",
    "redux": "^4.1.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.4",
    "typescript": "^4.5.2",
    "web-vitals": "^2.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "start-electron": "nodemon --watch ./electron/* --ext \"ts\" --exec \"tsc -p electron && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "dev": "concurrently --kill-others \"yarn start\" \"wait-on http://localhost:3000 && yarn start-electron\" ",
    "electron:build": "yarn build && tsc -p electron && electron-builder",
    "eject": "react-scripts eject"
  },
  "build": {
    "extraResources": [
      {
        "from": "./node_modules/node-adodb/lib/adodb.js",
        "to": "adodb.js"
      }
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "MarcosRosado",
        "repo": "sistema",
        "token": "",
        "private": true
      }
    ],
    "extends": null,
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "electron": "^16.0.0",
    "electron-builder": "^22.13.1",
    "wait-on": "^6.0.0"
  }
}
